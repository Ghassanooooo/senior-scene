{
  "name": "senior-scene",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "react",
    "vite",
    "shadcn",
    "tailwind",
    "css",
    "components",
    "senior-scene"
  ],
  "exports": "./dist/index.js",
  "files": [
    "/dist",
    "/build"
  ],
  "version": "1.2.04",
  "description": "create new react vite app with shadcn ui and tailwind css install the shadcn components and add base folders and files",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "echo 'Run npx senior-scene@latest init to start.'",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:cli": "tsup --watch",
    "build:cli": "tsup",
    "dev:start:cli": "concurrently \"pnpm dev:cli\" \"wait-on build/index.js && nodemon build/index.js\"",
    "start:cli": "node build/index.js"
  },
